{
    "intervallInMinutes": 30,
    "priceParser": [
        {
            "pattern": [
                {
                    "name": "BambuLab DE Format: € 1.234,56 EUR",
                    "matchPattern": "€\\s?([\\d.]+,\\d{2})\\sEUR"
                }
            ],
            "rules": [
                {
                    "pattern": "[^\\d.,-]",
                    "flags": "g",
                    "replaceWith": ""
                },
                {
                    "pattern": "\\.",
                    "flags": "g",
                    "replaceWith": ""
                },
                {
                    "pattern": ",",
                    "flags": "",
                    "replaceWith": "."
                }
            ]
        },
        {
            "pattern": [ 
                {
                    "name": "Amazon DE Format: 1.234,56 €",
                    "matchPattern": "\\d{1,3}(?:\\.\\d{3})*,\\d{2}\\s*€"
                },
                {
                    "name": "Idealo DE Format: 1.234,56 €",
                    "matchPattern": "ab\\s+(\\d+(?:\\.\\d{3})*,\\d{2})\\s*€"
                },
                {
                    "name": "€ 1.234,56 ",
                    "matchPattern": "€\\s*\\d{1,3}(?:\\.\\d{3})*,\\d{2}"
                }
            ],
            "rules": [
                {
                    "pattern": "[^\\d.,-]",
                    "flags": "g",
                    "replaceWith": ""
                },
                {
                    "pattern": "\\.",
                    "flags": "g",
                    "replaceWith": ""
                },
                {
                    "pattern": ",",
                    "flags": "",
                    "replaceWith": "."
                }
            ]
        }
    ]
}