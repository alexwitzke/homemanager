{
    "intervallInMinutes": 1,
    "priceParser": [
        {
            "pattern": [
                {
                    "name": "Flexibles Preisformat",
                    "matchPattern": "(?:€|\\$|EUR|USD)?\\s*[\\d\\s,.]+(?:[.,]\\d{1,2})?\\s*(?:€|\\$|EUR|USD)?\\b"
                }
            ],
            "rules": [
                {
                    "pattern": "\\s*(€|\\$|EUR?|USD?)\\s*",
                    "flags": "gi",
                    "replaceWith": ""
                },
                {
                    "pattern": "\\s+",
                    "flags": "g",
                    "replaceWith": ""
                },
                {
                    "pattern": "\\.(?=\\d{3})",
                    "flags": "g",
                    "replaceWith": ""
                },
                {
                    "pattern": ",",
                    "flags": "g",
                    "replaceWith": "."
                },
                {
                    "pattern": "^0+(?=\\d)",
                    "flags": "g",
                    "replaceWith": ""
                },
                {
                    "pattern": "^\\s+|\\s+$",
                    "flags": "g",
                    "replaceWith": ""
                }
            ]
        }
    ]
}